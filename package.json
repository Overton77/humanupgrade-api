    "start-admin": "tsx --watch src/restServer.ts",
    "create:admin": "tsx scripts/oneoffs/createAdmin.ts",
    "start:recommendations:worker": "tsx --watch src/workers/recommendations.worker.ts",
    "start:trending:worker": "tsx --watch src/workers/trending.worker.ts",
    "dev:all": "concurrently -k -n server,recommendations,trending -c blue,green,magenta \"pnpm dev\" \"pnpm start:recommendations:worker\" \"pnpm start:trending:worker\""
  },
