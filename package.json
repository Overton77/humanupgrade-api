  "scripts": {
    "dev": "tsx --watch src/server.ts",
    "create-vector-indices": "tsx src/scripts/createVectorIndices.ts",
    "build": "tsc && pnpm copy-schema",
    "copy-schema": "copyfiles -u 1 src/graphql/schema.graphql dist",
