    "@types/ws": "^8.18.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dataloader": "^2.2.3",
    "dotenv": "^17.2.3",
